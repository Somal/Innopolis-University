import java.awt.List;

public class main {
	public static void main(String[] args) {
		PointLinkedList pll = new PointLinkedList();
		pll.addPoint(0, 0);
		pll.show();
		pll.addPoint(1, 1);
		pll.show();
		pll.insert(1, 2, 2);
		pll.show();
		pll.set(2, 1.5, 1.5);
		pll.show();
	
	}
}
