
public class TreeTest {
	public static void main(String[] args) {
		MyTree<Integer> tree=new MyTree<Integer>(2);
		for (int i=0;i<15;i++){
			tree.add(i);
		}
		for (int i=0;i<10;i++){
			System.out.println("parent of "+i+" : "+tree.parentIndex(i));
		}
		
		for (int i=0;i<10;i++){
			System.out.println("of "+i+" child1: "+tree.childIndex(i,0)+" child2: "+tree.childIndex(i, 1));
		}
		
		tree.postorderTraversal(0);
		System.out.println();
		tree.preorderTraversal(0);
	}

}
